kBD;;EApCM,CAVI;;EAgDbU,OAAO,GAAG;IACRzB,KAAI,CACD0B,GADH,CAEI,uEAFJ,EAIGjB,IAJH,CAISC,QAAD,IAAc;MAClBM,OAAO,CAACC,GAAR,CAAYP,QAAQ,CAACR,IAArB;;MACA,KAAK,IAAIyB,GAAT,IAAgBjB,QAAQ,CAACR,IAAzB,EAA+B;QAC7B;QACAc,OAAO,CAACC,GAAR,CAAYP,QAAQ,CAACR,IAAT,CAAcyB,GAAd,CAAZ;QACA,KAAKvB,QAAL,CAAcO,IAAd,CAAmBD,QAAQ,CAACR,IAAT,CAAcyB,GAAd,CAAnB;QACA,IAAIC,IAAG,GAAI;UACTpB,IAAI,EAAEE,QAAQ,CAACR,IAAT,CAAcyB,GAAd,EAAmBnB,IADhB;UAETI,EAAE,EAAEe;QAFK,CAAX;QAIA,KAAKvB,QAAL,CAAcO,IAAd,CAAmBiB,IAAnB;MACF;IACD,CAhBH,EAiBGd,KAjBH;EAkBD;;AAnEY,CAAf","names":["Todo","axios","components","data","todoText","todoList","methods","addTodo","post","text","then","response","push","id","name","catch","e","console","log","deleteTodo","todoId","delete","index","findIndex","i","splice","created","get","key","todo"],"sourceRoot":"","sources":["/Users/hannan/Desktop/vue.js_to do list/src/App.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n    <div class=\"row\">\n      <div class=\"col-md-8 offset-md-2 text-center\">\n        <h3 class=\"mt-5\">Todo List | vue</h3>\n        <hr />\n        <div class=\"row\">\n          <div\n            class=\"col-md-6 offset-md-3 border d-flex flex-row justify-content-between align-items-center\"\n          >\n            <input v-model=\"todoText\" type=\"text\" />\n            <button @click=\"addTodo()\" class=\"btn btn-primary\">Ekle</button>\n          </div>\n        </div>\n        <hr />\n        <div class=\"todo-container\">\n          <Todo\n            @deleteTodo=\"deleteTodo($event)\"\n            v-for=\"todo in todoList\"\n            :key=\"todo.id\"\n            :todo=\"todo\"\n          />\n        </div>\n        {{ todoList.id }}\n      </div>\n    </div>\n    \n  </div>\n</template>\n\n<script>\nimport Todo from \"@/components/Todo\";\nimport axios from \"axios\";\nexport default {\n  components: {\n    Todo,\n  },\n  data() {\n    return {\n      todoText: \"\",\n      todoList: [],\n    };\n  },\n  methods: {\n    addTodo() {\n      axios\n        .post(\n          \"https://vuejs-1-video-6e184-default-rtdb.firebaseio.com/todoList.json\",\n          { text: this.todoText }\n        )\n        .then((response) => {\n          this.todoList.push({\n            id: response.data.name,\n            text: this.todoText,\n          });\n        })\n        .catch((e) => {\n          console.log(e);\n        });\n    },\n    deleteTodo(todoId) {\n      axios\n        .delete(\n          \"https://vuejs-1-video-6e184-default-rtdb.firebaseio.com/todoList/\" +\n            todoId +\n            \".json\"\n        )\n\n        .then((response) => {\n          let index = this.todoList.findIndex((i) => {\n            console.log(response)\n            return i.id == todoId;\n            \n          });\n          this.todoList.splice(index, 1);\n        })\n        .catch((e) => {\n          console.log(e);\n        });\n    },\n  },\n  created() {\n    axios\n      .get(\n        \"https://vuejs-1-video-6e184-default-rtdb.firebaseio.com/todoList.json\"\n      )\n      .then((response) => {\n        console.log(response.data);\n        for (let key in response.data) {\n          //console.log(response.data[key]);\n          console.log(response.data[key]);\n          this.todoList.push(response.data[key]);\n          let todo = {\n            text: response.data[key].text,\n            id: key,\n          };\n          this.todoList.push(todo);\n        }\n      })\n      .catch();\n  },\n};\n</script>\n"]}]}